<% content_for :cdn_code do %>
  <%= javascript_include_tag '//cdn.auth0.com/js/lock-7.11.min.js' %>
  <%= stylesheet_link_tag  'dashboard' %>
<% end %>
<%= render "layouts/header" %>
<div class="feed">
    </br></br>
    <div class="container container-logged-in">
      <div class="banner">
        <div class="row small-up-2 large-up-4">
          <div id="cal-heatmap">
            <h3 style="color: #FFF; float: left;">Calories Out</h3>
            <div class="chart">
            <script type="text/javascript">
              var cal = new CalHeatMap();
              cal.init({
                "domain": "month", 
                "subdomain": "x_day", 
                "start": new Date('05-01-2015'),
                "range": 12, 
                "tooltip": true, 
                "legend" : [1000, 1500, 2000, 2500],
                "itemName": ["calorie", "calories"],
                "dataType": "json",
                "data": "/dashboard/data", 
                  legendColors: {
                    min: "white",
                    max: "slateblue",
                    empty: "white"
                    // Will use the CSS for the missing keys
                  }
              });
            </script>
            </div>
            <h3 style="color: #FFF; float: left;">Calories In</h3>
            <div class="chart">
            <script type="text/javascript">
              var cal2 = new CalHeatMap();
              cal2.init({
                "domain": "month", 
                "subdomain": "x_day", 
                "start": new Date('05-01-2015'),
                "range": 12, 
                "tooltip": true, 
                "legend" : [1000, 1500, 2000, 2500],
                "itemName": ["calorie", "calories"],
                "dataType": "json",
                "data": "/dashboard/data", 
                  legendColors: {
                    min: "white",
                    max: "steelblue",
                    empty: "white"
                    // Will use the CSS for the missing keys
                  }
              });
            </script>
            </div>
          </div>
        </div>
      </div>
    </div>
